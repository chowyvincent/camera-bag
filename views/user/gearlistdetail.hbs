{{# each products }}
	<div class="row">
		{{# each this }}	
			<div class="col-sm-6 col-md-4">
				<div class="thumbnail">
				  	<img src={{this.imageUrl}} class="img-responsive" alt="...">
				  	<div class="caption">
				    	<h3>{{this.itemName}}</h3>
				    	<h3>{{this._id}}</h3>
				      	<div class="price pull-left">${{this.price}}</div>
				      	<div class="price pull-right">Qty: {{this.qty}}</div>
					</div>
				</div>
		    </div>
      	{{/each}}
	</div>
{{/each}}
