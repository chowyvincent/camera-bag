{{# each products }}
	<div class="row">
		{{# each this }}	
			<div class="col-sm-6 col-md-4">
				<div class="thumbnail">
				  	<img src={{this.image_url}} class="img-responsive" alt="...">
				  	<div class="caption">
				    	<h3>{{this.item}}</h3>
				    	<div class="clearfix">
					      	<div class="price pull-left">${{this.price}}</div>
					      	<a href="/add-to-cart/{{ this._id }}" class="btn btn-success pull-right" role="button">Add to Camera Bag</a>
				    	</div>
					</div>
				</div>
		    </div>
      	{{/each}}
	</div>
{{/each}}